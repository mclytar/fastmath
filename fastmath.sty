% ----------------
% \range
% ----------------
\newcommand{\@range}[3][,]{\ensuremath{%
	\def\sep@param@range{#1}%
	\def\end@param@range{#2}%
	\def\indexable@param@range{#3}%
	{\indexable@param@range{}}_{\start@param@range{}}{\sep@param@range{}}{\mid@param@range{}}{\sep@param@range{}}{\indexable@param@range{}}_{\end@param@range{}}%
}}
\newcommand{\@@range}[1][\ldots]{\ensuremath{%
	\def\mid@param@range{#1}%
	\@range%
}}
\newcommand{\@@@range}[1][1]{\ensuremath{%
	\def\start@param@range{#1}%
	\@@range%
}}
\newcommand{\range}{\@@@range}
